{
    "compilerOptions": {
        "outDir": "./bin",
        "allowJs": false,
        "target": "es6",
        "module": "CommonJS",
        "esModuleInterop": true,
        "inlineSources": true,
        "sourceMap": true,
        "resolveJsonModule": true
    },
    "include": [
        "./src/**/*"
    ],
    "exclude": [
        "./src/frontend/**/*"
    ],
}